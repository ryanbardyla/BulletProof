organism SimpleMath {
    fn main() {
        express "Testing basic math without function calls:";
        
        let result = 5;
        express "Direct assignment: 5 = ";
        synthesize result;
        
        let calc = 2 + 3;
        express "Basic addition: 2 + 3 = ";
        synthesize calc;
        
        let mult = 4 * 3;
        express "Basic multiplication: 4 * 3 = ";
        synthesize mult;
    }
}